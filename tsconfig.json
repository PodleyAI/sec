{
  "include": ["src/**/*"],
  "files": ["src/sec.ts"],
  "exclude": ["**/*.test.ts"],
  "compilerOptions": {
    "lib": ["dom", "ESNext"],
    "moduleResolution": "bundler",
    "moduleDetection": "force",
    "composite": true,
    "strict": true,
    "downlevelIteration": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "declarationMap": true,
    "types": ["@types/bun"],
    "outDir": "dist",
    "baseUrl": "./src",
    "rootDir": "./src",
    "jsx": "react-jsx",
    "module": "preserve",
    "target": "esnext"
  }
}
