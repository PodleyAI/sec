{
  "name": "@podley/sec",
  "type": "module",
  "version": "0.0.1",
  "description": "Podley SEC is an example of using the Podley AI library to build a tool for retrieving SEC data.",
  "scripts": {
    "dev": "concurrently -c 'auto' -n 'sec:' 'bun:dev-*'",
    "dev-js": "bun build --watch --target=bun --sourcemap=external --packages=external --outdir ./dist ./src/sec.ts",
    "dev-types": "tsc --watch --preserveWatchOutput",
    "build-example": "bun run build-clean && bun run build-js",
    "build-clean": "rm -fr dist/*",
    "build-js": "bun build --target=bun --packages=external --outdir ./dist ./src/sec.ts",
    "build-types": "rm -f tsconfig.tsbuildinfo && tsc",
    "test": "bun test"
  },
  "bin": "./src/sec.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@podley/cli": "link:@podley/cli",
    "@podley/job-queue": "link:@podley/job-queue",
    "@podley/sqlite": "link:@podley/sqlite",
    "@podley/storage": "link:@podley/storage",
    "@podley/task-graph": "link:@podley/task-graph",
    "@podley/tasks": "link:@podley/tasks",
    "@podley/util": "link:@podley/util",
    "@sinclair/typebox": "^0.34.33",
    "@sroussey/parse-address": "^2.4.0",
    "awesome-phonenumber": "^7.5.0",
    "bun-types": "1.2.14",
    "cheerio": "^1.0.0",
    "cheerio-json-mapper": "^1.0.4",
    "commander": "^14.0.0",
    "compromise": "^14.14.4",
    "concurrently": "^9.1.2",
    "csv-parse": "^5.6.0",
    "fast-xml-parser": "^5.2.3",
    "html-entities": "^2.6.0",
    "pdf2json": "^3.1.6",
    "words-to-numbers": "^1.5.1"
  },
  "devDependencies": {
    "@types/csv-parse": "^1.2.5"
  }
}
