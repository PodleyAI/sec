{
  "name": "@ellmers/sec",
  "type": "module",
  "version": "0.0.1",
  "description": "Ellmers is a tool for building and running DAG pipelines of AI tasks.",
  "scripts": {
    "dev": "concurrently -c 'auto' -n 'sec:' 'bun:dev-*'",
    "dev-js": "bun build --watch --target=bun --sourcemap=external --packages=external --outdir ./dist ./src/sec.ts",
    "dev-types": "tsc --watch --preserveWatchOutput",
    "build-example": "bun run build-clean && bun run build-js",
    "build-clean": "rm -fr dist/*",
    "build-js": "bun build --target=bun --packages=external --outdir ./dist ./src/sec.ts",
    "build-types": "rm -f tsconfig.tsbuildinfo && tsc",
    "test": "bun test"
  },
  "bin": "./src/sec.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@ellmers/ai": "workspace:*",
    "@ellmers/ai-provider": "workspace:*",
    "@ellmers/job-queue": "workspace:*",
    "@ellmers/sqlite": "workspace:*",
    "@ellmers/storage": "workspace:*",
    "@ellmers/task-graph": "workspace:*",
    "@ellmers/tasks": "workspace:*",
    "@ellmers/test": "workspace:*",
    "@ellmers/util": "workspace:*",
    "@ellmers/cli": "workspace:*",
    "@sroussey/parse-address": "^2.4.0",
    "@types/csv-parse": "^1.2.5",
    "awesome-phonenumber": "^7.4.0",
    "cheerio": "^1.0.0",
    "cheerio-json-mapper": "^1.0.4",
    "commander": "^13.1.0",
    "compromise": "^14.14.4",
    "csv-parse": "^5.6.0",
    "fast-xml-parser": "^5.0.9",
    "html-entities": "^2.5.2",
    "pdf2json": "^3.1.5",
    "words-to-numbers": "^1.5.1"
  },
  "devDependencies": {
    "@types/csv-parse": "^1.2.5"
  }
}
